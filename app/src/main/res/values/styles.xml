<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:textColor">#000000</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="MyTabLayoutTextAppearance" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">35dp</item>
    </style>

    <style name="popwin_anim_style">
        <item name="android:windowEnterAnimation">@anim/previewanim</item>
        <item name="android:windowExitAnimation">@anim/previewoutanim</item>
    </style>
    <style name="GYCS_BaseTextViewStyle">
        <item name="android:textAlignment">center</item>
        <item name="android:background">@drawable/black_bord_shape</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="GYCS_BaseTextViewStyle2" parent="GYCS_BaseTextViewStyle">
        <item name="android:layout_span">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="GYCS_BaseTextViewStyle3">
        <item name="android:textAlignment">center</item>
        <item name="android:layout_span">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="GYCS_BaseTextViewStyle4" parent="GYCS_BaseTextViewStyle">
        <item name="android:layout_span">1</item>
        <item name="android:background">@drawable/black_bord_shape2</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="GYCS_BaseTextViewStyle5">
        <item name="android:textSize">10sp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_vertical|right</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_width">match_parent</item>
    </style>
    <style name="GYCS_BaseTextViewStyle6" >
        <item name="android:textSize">13sp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/black_bord_shape</item>
    </style>
    <style name="GYCS_BaseTextViewStyle6_2" >
        <item name="android:textSize">11sp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/black_bord_shape2</item>
    </style>
    <style name="GYCS_BaseTextViewStyle7" >
        <item name="android:layout_weight">1</item>
        <item name="android:textAlignment">center</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/black_bord_shape</item>

    </style>

    <style name="GYCS_BaseTextViewStyle8" >
        <item name="android:textSize">7sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/black_bord_shape</item>
    </style>

</resources>
